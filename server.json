{
  "log": {
    "loglevel": "warning"
  },
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "ip": [
          "ext:ir.dat:ir"
        ],
        "outboundTag": "block"
      },
      {
        "type": "field",
        "outboundTag": "block",
        "domain": [
          "regexp:\\.ir$"
        ]
      },
      {
        "type": "field",
        "ip": [
          "0.0.0.0/0"
        ],
        "outboundTag": "warp-IPv4"
      },
      {
        "type": "field",
        "ip": [
          "::/0"
        ],
        "outboundTag": "warp-IPv6"
      }
    ]
  },
  "inbounds": [
    {
      "listen": "0.0.0.0",
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "xx",
            "flow": "xtls-rprx-vision"
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "tcp",
        "security": "reality",
        "realitySettings": {
          "dest": "xx:443",
          "serverNames": [
            "xx"
          ],
          "privateKey": "xx",
          "shortIds": [
            "xx"
          ]
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls",
          "quic"
        ]
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "direct"
    },
    {
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIPv4"
      },
      "proxySettings": {
        "tag": "wireguard"
      },
      "tag": "warp-IPv4"
    },
    {
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIPv6"
      },
      "proxySettings": {
        "tag": "wireguard"
      },
      "tag": "warp-IPv6"
    },
    {
      "protocol": "wireguard",
      "settings": {
        "secretKey": "",
        "address": [
          "172.16.0.2/32",
          "/128"
        ],
        "peers": [
          {
            "publicKey": "bmXOC+F1FxEMF9dyiK2H5/1SUtzH0JuVo51h2wPfgyo=",
            "allowedIPs": [
              "0.0.0.0/0",
              "::/0"
            ],
            "endpoint": "162.159.192.1:2408"
          }
        ],
        "reserved": [
          0,
          0,
          0
        ],
        "mtu": 1280
      },
      "tag": "wireguard"
    },
    {
      "protocol": "blackhole",
      "tag": "block"
    }
  ]
}
